{
  "name": "stilya",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "NODE_ENV=test NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" jest --config jest.config.js",
    "test:ci": "NODE_ENV=test NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" JEST_WORKER_ID=1 jest --no-warnings --config jest.config.js",
    "test:basic": "NODE_ENV=test NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" jest --config jest.config.js src/__tests__/basic.test.js",
    "test:simple": "NODE_ENV=test NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" jest --config jest.config.js src/__tests__/simple.test.js",
    "test:authstore": "NODE_ENV=test NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" jest --config jest.config.js src/__tests__/auth/authStore.test.ts",
    "test:optional": "NODE_ENV=test NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" jest --config jest.config.js --testMatch \"<rootDir>/src/__tests__/optional/**/*.test.{js,jsx,ts,tsx}\"",
    "test:all": "npm run test:basic && npm run test:simple && npm run test:authstore && npm run test:optional",
    "test:summary": "bash ./scripts/summarize-test-results.sh",
    "test:ci:all": "npm run test:ci && npm run test:optional && npm run test:summary",
    "test:fix-uuid": "chmod +x ./scripts/fix-uuid-tests.sh && ./scripts/fix-uuid-tests.sh",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc",
    "clean": "rm -rf node_modules/.cache .expo/cache .metro-cache",
    "setup": "npm install && npm run clean",
    "fix-metro": "sh ./scripts/fix-metro-dependencies.sh",
    "fix-babel": "npm install --no-save @babel/runtime && npm dedupe @babel/runtime",
    "build:local": "sh ./scripts/local-android-build.sh",
    "build:direct": "chmod +x ./scripts/direct-eas-build.sh && ./scripts/direct-eas-build.sh",
    "test:build-fixes": "chmod +x ./scripts/test-build-fixes.sh && ./scripts/test-build-fixes.sh",
    "prebuild": "expo prebuild --clean",
    "eas:whoami": "eas whoami",
    "eas:build": "eas build --no-wait",
    "eas:build:dev": "eas build --profile development --no-wait",
    "eas:build:preview": "eas build --profile preview --no-wait",
    "eas:build:prod": "eas build --profile production --no-wait",
    "eas:update": "eas update",
    "eas:validate": "npx jsonlint-cli eas.json && npx jsonlint-cli app.json",
    "eas:build:ci": "EAS_SKIP_JAVASCRIPT_BUNDLING=1 eas build --platform android --profile ci --non-interactive --no-wait",
    "github:build": "EAS_SKIP_JAVASCRIPT_BUNDLING=1 eas build --platform android --profile production --non-interactive",
    "postinstall": "npm install --no-save @babel/runtime@7.27.1 && npm dedupe && (test -d patches && patch-package || echo 'パッチディレクトリが存在しません - パッチ適用をスキップします') && echo 'Dependencies configured & patches applied'",
    "fix-test": "chmod +x ./scripts/patch-react-native-jest.sh && ./scripts/patch-react-native-jest.sh",
    "fix-jest-expo": "chmod +x ./scripts/patch-jest-expo.sh && ./scripts/patch-jest-expo.sh",
    "install-test-deps": "chmod +x ./scripts/install-test-dependencies.sh && ./scripts/install-test-dependencies.sh",
    "prepare-test": "npm run install-test-deps && npm run fix-test && npm run fix-jest-expo && ./scripts/fix-expo-modules-for-jest.sh"
  },
  "dependencies": {
    "@babel/runtime": "7.27.1",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/netinfo": "11.1.0",
    "@react-navigation/bottom-tabs": "^6.5.16",
    "@react-navigation/native": "^6.1.14",
    "@react-navigation/native-stack": "^6.9.22",
    "@react-navigation/stack": "^6.3.25",
    "@supabase/supabase-js": "^2.39.3",
    "axios": "^1.6.7",
    "expo": "~53.0.0",
    "expo-application": "~6.1.0",
    "expo-crypto": "14.1.4",
    "expo-device": "~7.1.0",
    "expo-file-system": "18.1.10",
    "expo-image": "~2.1.0",
    "expo-linking": "~7.1.0",
    "expo-localization": "~16.1.0",
    "expo-notifications": "~0.31.0",
    "expo-secure-store": "~14.2.0",
    "expo-status-bar": "~2.2.0",
    "nativewind": "^4.0.1",
    "path-browserify": "1.0.1",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "4.10.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-url-polyfill": "^2.0.0",
    "stream-browserify": "3.0.0",
    "tailwind-merge": "^3.0.0",
    "url": "0.11.4",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@babel/plugin-transform-react-jsx": "7.27.1",
    "@babel/plugin-transform-runtime": "7.27.1",
    "@babel/preset-env": "7.27.2",
    "@expo/config": "^11.0.0",
    "@expo/config-plugins": "~10.0.0",
    "@expo/metro-config": "0.9.0",
    "@expo/prebuild-config": "~9.0.0",
    "@jest/globals": "29.7.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/react-native": "^12.3.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "babel-plugin-module-resolver": "5.0.2",
    "babel-plugin-transform-imports": "2.0.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-preset-expo": "~13.1.11",
    "eas-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "29.7.0",
    "jest-expo": "~53.0.0",
    "jest-junit": "^16.0.0",
    "jest-serializer-path": "0.1.15",
    "metro": "0.77.0",
    "metro-cache": "0.77.0",
    "metro-config": "0.77.0",
    "metro-minify-terser": "0.77.0",
    "metro-transform-worker": "0.77.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.2.5",
    "react-test-renderer": "19.1.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.0",
    "uuid": "11.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "resolutions": {
    "@babel/runtime": "7.27.1",
    "metro": "0.77.0",
    "metro-config": "0.77.0",
    "@expo/metro-config": "0.9.0",
    "babel-preset-expo": "~13.1.11"
  },
  "private": true
}
