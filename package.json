{
  "name": "stilya",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "description": "パーソナライズ型ファッション提案アプリ",
  "scripts": {
    "start": "expo start",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "prebuild": "expo prebuild",
    "lint": "eslint src/ --ext .ts,.tsx --max-warnings 10",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test:ci": "echo 'No tests configured yet'",
    "clean": "rm -rf node_modules/.cache .expo .metro-cache",
    "reset": "npm run clean && rm -rf node_modules && npm install",
    "eas:build:android": "eas build --platform android --profile preview",
    "eas:build:ios": "eas build --platform ios --profile preview"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "2.1.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^6.5.16",
    "@react-navigation/native": "^6.1.14",
    "@react-navigation/native-stack": "^6.9.22",
    "@react-navigation/stack": "^6.3.25",
    "@supabase/supabase-js": "^2.39.3",
    "axios": "^1.6.7",
    "expo": "~53.0.0",
    "expo-application": "~6.0.1",
    "expo-crypto": "~14.0.1",
    "expo-device": "~7.0.1",
    "expo-file-system": "~18.0.4",
    "expo-image": "~2.0.1",
    "expo-linking": "~7.0.3",
    "expo-localization": "~16.0.1",
    "expo-notifications": "~0.30.1",
    "expo-secure-store": "~14.0.0",
    "expo-status-bar": "~2.0.0",
    "expo-updates": "~0.28.13",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.22.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.14.0",
    "react-native-screens": "~4.4.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-url-polyfill": "^2.0.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/react": "~18.2.0",
    "@types/react-native": "0.74.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "10.4.21",
    "babel-plugin-module-resolver": "5.0.2",
    "babel-preset-expo": "~11.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^11.2.0",
    "postcss": "8.5.3",
    "prettier": "^3.2.5",
    "tailwindcss": "3.3.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "private": true,
  "keywords": [
    "react-native",
    "expo",
    "fashion",
    "mobile-app",
    "typescript"
  ],
  "author": "Stilya Development Team",
  "license": "UNLICENSED"
