{
  "name": "stilya",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "NODE_ENV=test NODE_OPTIONS=\"--no-warnings\" jest --config jest.config.js",
    "test:ci": "NODE_ENV=test NODE_OPTIONS=\"--no-warnings\" jest --config jest.config.js src/__tests__/simple.test.js",
    "test:simple": "NODE_ENV=test NODE_OPTIONS=\"--no-warnings\" jest --config jest.config.js src/__tests__/simple.test.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc",
    "clean": "rm -rf node_modules/.cache .expo/cache .metro-cache",
    "setup": "npm install && npm run clean",
    "fix-metro": "sh ./scripts/fix-metro-dependencies.sh",
    "fix-babel": "npm install --no-save @babel/runtime && npm dedupe @babel/runtime",
    "build:local": "sh ./scripts/local-android-build.sh",
    "prebuild": "expo prebuild --clean",
    "eas:whoami": "eas whoami",
    "eas:build": "eas build --no-wait",
    "eas:build:dev": "eas build --profile development --no-wait",
    "eas:build:preview": "eas build --profile preview --no-wait",
    "eas:build:prod": "eas build --profile production --no-wait",
    "eas:update": "eas update",
    "eas:validate": "npx jsonlint-cli eas.json && npx jsonlint-cli app.json",
    "eas:build:ci": "EAS_NO_METRO=true EXPO_NO_CACHE=true EAS_SKIP_JAVASCRIPT_BUNDLING=1 EXPO_NO_BUNDLER=1 eas build --platform android --profile ci --non-interactive --no-wait",
    "github:build": "EAS_NO_METRO=true EXPO_NO_CACHE=true EAS_SKIP_JAVASCRIPT_BUNDLING=1 EXPO_NO_BUNDLER=1 eas build --platform android --profile production --non-interactive",
    "postinstall": "npm install --no-save @babel/runtime && npm dedupe @babel/runtime && echo 'Dependencies configured'"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.0",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-community/netinfo": "^11.0.0",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.0.0",
    "@react-navigation/stack": "^7.0.0",
    "@supabase/supabase-js": "^2.39.3",
    "axios": "^1.6.7",
    "expo": "~53.0.0",
    "expo-application": "~6.1.0",
    "expo-device": "~7.1.0",
    "expo-image": "~2.1.0",
    "expo-linking": "~7.1.0",
    "expo-localization": "~16.1.0",
    "expo-notifications": "~0.31.0",
    "expo-secure-store": "~14.2.0",
    "expo-status-bar": "~2.2.0",
    "nativewind": "^4.0.1",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-url-polyfill": "^2.0.0",
    "tailwind-merge": "^3.0.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@expo/config": "^11.0.0",
    "@expo/config-plugins": "~10.0.0",
    "@expo/metro-config": "^0.10.7",
    "@expo/prebuild-config": "~9.0.0",
    "@jest/globals": "29.7.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.3.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-preset-expo": "~13.0.0",
    "eas-cli": "^7.8.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-expo": "~53.0.0",
    "metro": "^0.76.8",
    "metro-config": "^0.76.8",
    "metro-minify-terser": "^0.76.8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "resolutions": {
    "@babel/runtime": "^7.24.0",
    "metro": "^0.76.8",
    "metro-config": "^0.76.8"
  },
  "private": true
}