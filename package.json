{
  "name": "stilya",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "fix-all": "./fix-dependencies.sh",
    "test": "NODE_ENV=test NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" jest --config jest.config.js",
    "test:ci": "NODE_ENV=test NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" JEST_WORKER_ID=1 jest --no-warnings --config jest.config.js",
    "test:basic": "NODE_ENV=test NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" jest --config jest.config.js src/__tests__/basic.test.js",
    "test:simple": "NODE_ENV=test NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" jest --config jest.config.js src/__tests__/simple.test.js",
    "test:authstore": "NODE_ENV=test NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" jest --config jest.config.js src/__tests__/auth/authStore.test.ts",
    "test:optional": "NODE_ENV=test NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" jest --config jest.config.js --testMatch \"<rootDir>/src/__tests__/optional/**/*.test.{js,jsx,ts,tsx}\"",
    "test:all": "npm run test:basic && npm run test:simple && npm run test:authstore && npm run test:optional",
    "test:summary": "bash ./scripts/summarize-test-results.sh",
    "test:ci:all": "npm run test:ci && npm run test:optional && npm run test:summary",
    "test:fix-uuid": "chmod +x ./scripts/fix-uuid-tests.sh && ./scripts/fix-uuid-tests.sh",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc",
    "clean": "rm -rf node_modules/.cache .expo/cache .metro-cache",
    "setup": "npm install && npm run clean",
    "fix-metro": "bash ./scripts/fix-metro-dependencies.sh",
    "fix-metro-compatibility": "chmod +x ./scripts/fix-metro-incompatibility.sh && ./scripts/fix-metro-incompatibility.sh",
    "fix-metro-emergency": "chmod +x ./scripts/fix-metro-serializer-direct.sh && ./scripts/fix-metro-serializer-direct.sh",
    "fix-babel": "npm install --no-save @babel/runtime && npm dedupe @babel/runtime",
    "build:local": "bash ./scripts/local-android-build.sh",
    "build:local:release": "EXPO_NO_CACHE=true NODE_ENV=production bash ./scripts/local-android-build.sh",
    "build:direct": "chmod +x ./scripts/direct-eas-build.sh && ./scripts/direct-eas-build.sh",
    "test:build-fixes": "chmod +x ./scripts/test-build-fixes.sh && ./scripts/test-build-fixes.sh",
    "prebuild": "expo prebuild --clean",
    "prebuild:compatibility": "npm run fix-metro-compatibility && expo prebuild --clean",
    "eas:whoami": "eas whoami",
    "eas:build": "eas build --no-wait",
    "eas:build:dev": "eas build --profile development --no-wait",
    "eas:build:preview": "eas build --profile preview --no-wait",
    "eas:build:prod": "eas build --profile production --no-wait",
    "eas:build:local": "eas build --platform android --profile local --local",
    "eas:update": "eas update",
    "eas:validate": "npx jsonlint-cli eas.json && npx jsonlint-cli app.json",
    "create-terminal-reporter": "chmod +x ./scripts/create-terminal-reporter.sh && ./scripts/create-terminal-reporter.sh",
    "create-metro-core": "chmod +x ./scripts/create-metro-core.sh && ./scripts/create-metro-core.sh",
    "setup:ci": "npm run fix-metro-compatibility && npm run create-terminal-reporter && npm run create-metro-core",
    "eas:build:ci": "npm run setup:ci && EAS_SKIP_JAVASCRIPT_BUNDLING=1 eas build --platform android --profile ci --non-interactive --no-wait",
    "build:fixed": "bash ./scripts/fixed-android-build.sh",
    "build:fixed:v2": "bash ./scripts/fixed-android-build-v2.sh",
    "build:github-actions": "npm run setup:ci && npm run build:local:release",
    "fix:metro-all": "npm run fix-metro && npm run create-terminal-reporter && npm run fix-metro-compatibility",
    "eas:prep": "bash ./scripts/eas-build-prepare.sh",
    "eas:debug": "bash ./scripts/eas-build-debug.sh",
    "eas:fix": "bash ./scripts/ci-build-fix.sh",
    "github:build": "npm run setup:ci && EAS_SKIP_JAVASCRIPT_BUNDLING=1 eas build --platform android --profile ci --non-interactive",
    "github:build:local": "EAS_SKIP_JAVASCRIPT_BUNDLING=1 NODE_ENV=production bash ./scripts/local-android-build.sh",
    "postinstall": "npm install --no-save @babel/runtime@7.27.1 && npm dedupe && echo 'Dependencies configured'",
    "fix-test": "chmod +x ./scripts/patch-react-native-jest.sh && ./scripts/patch-react-native-jest.sh",
    "fix-jest-expo": "chmod +x ./scripts/patch-jest-expo.sh && ./scripts/patch-jest-expo.sh",
    "fix-missing-versions": "node ./scripts/fix-missing-versions.js",
    "install:ci": "yarn install && node ./scripts/fix-missing-versions.js",
    "prepare-test": "npm run install-test-deps && npm run fix-test && npm run fix-jest-expo && ./scripts/fix-expo-modules-for-jest.sh",
    "bundle": "node --max_old_space_size=4096 node_modules/expo/AppEntry.js",
    "create-bundle-helpers": "chmod +x ./scripts/create-bundle-helpers.sh && ./scripts/create-bundle-helpers.sh",
    "prebuild-and-bundle": "npm run create-bundle-helpers && npm run prebuild",
    "fix-expo-metro": "npm run fix-metro && npm run create-bundle-helpers",
    "fix:metro-config": "npm install --save-dev @expo/metro-config@0.9.0 --force && npm dedupe",
    "fix:deps": "npm install --force && npm run fix:metro-config && npm run fix-babel",
    "fix:ci-build": "npm run setup:ci && npm run clean && npm run fix:metro-all && npm run github:build:local",
    "fix:metro-ci": "chmod +x ./scripts/fix-metro-for-ci.sh && ./scripts/fix-metro-for-ci.sh",
    "test:github-actions": "chmod +x ./scripts/test-github-actions-fix.sh && ./scripts/test-github-actions-fix.sh"
  },
  "dependencies": {
    "@babel/runtime": "7.27.1",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/netinfo": "11.1.0",
    "@react-navigation/bottom-tabs": "^6.5.16",
    "@react-navigation/native": "^6.1.14",
    "@react-navigation/native-stack": "^6.9.22",
    "@react-navigation/stack": "^6.3.25",
    "@supabase/supabase-js": "^2.39.3",
    "axios": "^1.6.7",
    "expo": "~53.0.0",
    "expo-application": "~6.1.0",
    "expo-crypto": "14.1.4",
    "expo-dev-client": "~5.1.8",
    "expo-device": "~7.1.0",
    "expo-file-system": "18.1.10",
    "expo-image": "~2.1.0",
    "expo-linking": "~7.1.0",
    "expo-localization": "~16.1.0",
    "expo-notifications": "~0.31.0",
    "expo-secure-store": "~14.2.0",
    "expo-status-bar": "~2.2.0",
    "expo-updates": "~0.28.13",
    "nativewind": "^4.0.1",
    "path-browserify": "1.0.1",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "4.10.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-url-polyfill": "^2.0.0",
    "stream-browserify": "3.0.0",
    "tailwind-merge": "^3.0.0",
    "url": "0.11.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@babel/plugin-transform-react-jsx": "7.27.1",
    "@babel/plugin-transform-runtime": "7.27.1",
    "@babel/preset-env": "7.27.2",
    "@expo/config": "^11.0.0",
    "@expo/config-plugins": "~10.0.0",
    "@expo/metro-config": "0.9.0",
    "@expo/prebuild-config": "~9.0.0",
    "@jest/globals": "29.7.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/react-native": "^12.3.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "babel-plugin-module-resolver": "5.0.2",
    "babel-plugin-transform-imports": "2.0.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-preset-expo": "13.1.11",
    "eas-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "29.7.0",
    "jest-expo": "~53.0.0",
    "jest-junit": "^16.0.0",
    "jest-serializer-path": "0.1.15",
    "metro": "0.77.0",
    "metro-cache": "0.77.0",
    "metro-config": "0.77.0",
    "metro-core": "0.77.0",
    "metro-minify-terser": "0.77.0",
    "metro-runtime": "0.77.0",
    "metro-source-map": "0.77.0",
    "metro-transform-worker": "0.77.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.2.5",
    "react-test-renderer": "19.1.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.0",
    "uuid": "11.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "resolutions": {
    "@babel/runtime": "7.27.1",
    "metro": "0.77.0",
    "metro-config": "0.77.0",
    "metro-cache": "0.77.0",
    "metro-core": "0.77.0", 
    "metro-minify-terser": "0.77.0",
    "metro-source-map": "0.77.0",
    "metro-runtime": "0.77.0",
    "metro-resolver": "0.77.0",
    "metro-transform-worker": "0.77.0",
    "@expo/metro-config": "0.9.0",
    "babel-preset-expo": "13.1.11",
    "rimraf": "^3.0.2",
    "glob": "^9.3.5",
    "core-js": "^3.30.1"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "private": true
}
