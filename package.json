{
  "name": "stilya",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "ios": "expo start --ios",
    "android": "expo start --android",
    "clean": "rm -rf node_modules .expo",
    "reset": "npm run clean && npm install",
    "test": "node scripts/run-tests.js",
    "test:expo": "echo 'Run tests in Expo by importing runLocalTests in App.tsx'",
    "eas-build-preview": "eas build --platform all --profile preview",
    "eas-build-production": "eas build --platform all --profile production",
    "clear-cache": "npx expo start --clear",
    "check-env": "node -e \"console.log('SUPABASE_URL:', process.env.EXPO_PUBLIC_SUPABASE_URL ? 'Set' : 'Not set')\"",
    "full-reset": "./scripts/full-reset.sh"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "~2.1.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^6.5.16",
    "@react-navigation/native": "^6.1.14",
    "@react-navigation/native-stack": "^6.9.22",
    "@react-navigation/stack": "7.3.2",
    "@supabase/supabase-js": "^2.39.3",
    "axios": "1.9.0",
    "expo": "~53.0.0",
    "expo-constants": "~17.0.0",
    "expo-image": "2.1.7",
    "expo-linking": "~7.0.0",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.20.0",
    "react-native-reanimated": "3.10.1",
    "react-native-safe-area-context": "~4.14.0",
    "react-native-screens": "~4.4.0",
    "react-native-toast-message": "2.3.0",
    "react-native-url-polyfill": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/react": "~18.3.0",
    "babel-plugin-module-resolver": "5.0.2",
    "babel-preset-expo": "~12.0.0",
    "dotenv": "16.5.0",
    "metro": "~0.81.0",
    "metro-config": "~0.81.0",
    "metro-core": "~0.81.0",
    "typescript": "^5.3.0"
  },
  "private": true
}
