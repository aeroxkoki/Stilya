{
  "name": "stilya",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:prod": "expo start --no-dev",
    "dev": "expo start",
    "ios": "expo start --ios",
    "android": "expo start --android",
    "clean": "rm -rf node_modules .expo",
    "reset": "npm run clean && npm install",
    "test": "echo 'Testing scripts have been cleaned up'",
    "test:expo": "echo 'Run tests in Expo by importing runLocalTests in App.tsx'",
    "eas-build-preview": "eas build --platform all --profile preview",
    "eas-build-production": "eas build --platform all --profile production",
    "eas-build-development": "eas build --platform all --profile development",
    "clear-cache": "npx expo start --clear",
    "check-env": "node -e \"console.log('SUPABASE_URL:', process.env.EXPO_PUBLIC_SUPABASE_URL ? 'Set' : 'Not set')\"",
    "start:expo-go": "expo start",
    "full-reset": "./scripts/maintenance/full-clean.sh",
    "sync-products": "node scripts/sync/sync-mvp-brands.js",
    "types:generate": "./scripts/utils/generate-types.sh",
    "types:check": "tsc --noEmit",
    "monitor:capacity": "node scripts/monitoring/supabase-free-tier-monitor.js",
    "cleanup:dry-run": "node scripts/maintenance/smart-deletion-manager.js --dry-run",
    "cleanup:execute": "node scripts/maintenance/smart-deletion-manager.js",
    "cleanup:emergency": "node scripts/maintenance/emergency-deletion.js",
    "db:migrate": "node scripts/database/migrate-schema.js",
    "db:check": "node scripts/utils/check-products.ts",
    "patch:daily": "node scripts/maintenance/simple-daily-patch.js",
    "optimize:performance": "node scripts/optimize-performance.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "~2.1.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^6.5.16",
    "@react-navigation/native": "^6.1.14",
    "@react-navigation/native-stack": "^6.9.22",
    "@react-navigation/stack": "^6.4.1",
    "@supabase/supabase-js": "^2.50.2",
    "axios": "^1.9.0",
    "expo": "53.0.0",
    "expo-constants": "~17.1.6",
    "expo-haptics": "^14.1.4",
    "expo-image": "~2.1.7",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.5",
    "expo-network": "^7.1.5",
    "expo-status-bar": "~2.2.3",
    "node-fetch": "^2.7.0",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.12.0",
    "react-native-toast-message": "^2.3.0",
    "react-native-url-polyfill": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/react": "~19.0.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "dotenv": "^16.6.0",
    "typescript": "^5.3.0"
  },
  "private": true
}
