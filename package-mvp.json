{
  "name": "stilya",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "description": "パーソナライズ型ファッション提案アプリ - MVP",
  "scripts": {
    "start": "expo start",
    "ios": "expo run:ios",
    "android": "expo run:android",
