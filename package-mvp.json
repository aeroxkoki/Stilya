    "@typescript-eslint/parser": "^6.19.0",
    "babel-preset-expo": "~11.0.0",
    "eslint": "^8.57.1",
    "prettier": "^3.2.5",
    "tailwindcss": "3.3.0",
    "typescript": "^5.3.0"
  },
  "expo": {
    "install": {
      "exclude": ["expo-dev-client", "expo-notifications", "expo-updates"]
