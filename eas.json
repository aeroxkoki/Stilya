{
  "cli": {
    "version": ">=16.0.0"
  },
  "build": {
    "ci": {
      "developmentClient": false,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EAS_SKIP_JAVASCRIPT_BUNDLING": "1"
      },
      "autoIncrement": true,
      "runtimeVersion": {
        "policy": "sdkVersion"
      }
    },
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "developmentClient": true,
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
