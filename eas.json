{
  "cli": {
    "version": ">=7.3.0",
    "requireCommit": false,
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "ci": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EAS_SKIP_JAVASCRIPT_BUNDLING": "1",
        "EXPO_NO_CACHE": "1",
        "NODE_ENV": "production"
      },
      "node": "20",
      "autoIncrement": false
    }
  },
  "submit": {
    "production": {}
  }
}